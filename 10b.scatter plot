import matplotlib.pyplot as plt

months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
rainfall = [50, 45, 60, 80, 100, 120, 130, 130, 110, 90, 70, 55]

plt.scatter(months, rainfall, marker='o', color='b')
plt.title('Monthly Rainfall Over the Year')
plt.xlabel('Month')
plt.ylabel('Rainfall (mm)')


plt.show()
